{

  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "alt-editor.binary": "subl",
  "alt-editor.name": "sublime",
  "bracket-pair-colorizer-2.forceUniqueOpeningColor": true,
  "bracket-pair-colorizer-2.colors": [
    "Gold",
    "Orchid",
    "LightSkyBlue",
    "GreenYellow",
    "FireBrick",
    "Turquoise",
    "OliveDrab"
  ],
  "breadcrumbs.enabled": true,
  "editor.autoIndent": "full",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.cursorStyle": "block",
  "editor.fontFamily": "'Fira Code', 'Fira Mono for Powerline', 'Source Code Pro for Powerline', 'Droid Sans Mono Dotted for Powerline', 'Ubuntu Mono derivative Powerline', 'Consolas', 'Courier New', 'monospace'",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": false,
  "editor.insertSpaces": true,
  "editor.lineHeight": 16,
  "editor.lineNumbers": "on",
  "editor.linkedEditing": true,
  "editor.matchBrackets": "near",
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": true,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "gutter",
  // "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80,
    120
  ],
  "editor.semanticHighlighting.enabled": "configuredByTheme",
  "editor.tabSize": 4,
  "editor.tokenColorCustomizations": {
    "[Material Theme Darker High Contrast]": {
      "comments": "#888",
      "textMateRules": [
        {
          "name": "Comment punctuation",
          "scope": "punctuation.definition.comment",
          "settings": {
            "foreground": "#888"
          }
        }
      ]
    }
  },
  "editor.wordWrap": "on",
  "explorer.compactFolders": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "emmet.syntaxProfiles": { "javascript": "jsx" },
  "emmet.includeLanguages": { "javascript": "javascriptreact" },
  "files.associations": {
    ".prettierrc": "json",
    ".sequelizerc": "javascript",
    ".stylelintrc": "json"
  },
  "files.autoSave": "off",
  "files.autoSaveDelay": 10000,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.hotExit": "onExit",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true
  },
  "git.ignoreMissingGitWarning": true,
  "http.proxyStrictSSL": false,
  "multiCommand.commands": [
    {
      "command": "multiCommand.makeRoom",
      "sequence": [
        "workbench.action.toggleSidebarVisibility",
        "workbench.action.toggleActivityBarVisibility"
      ]
    }
  ],
  "terminal.integrated.shell.windows": "C:\\Program Files (x86)\\Git\\bin\\bash.exe",
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "window.restoreWindows": "all",
  "window.title": "${rootName}${separator}${activeEditorLong} //vscode",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Night Wolf [dark gray]",
  "workbench.colorCustomizations": {
    // "editorIndentGuide.background": "#0000ff",
    "editorCursor.background": "#662266",
    "editorCursor.foreground": "#dd88ff",
    "editor.findMatchBackground": "#00cc44a8", //Current SEARCH MATCH
    "editor.findMatchHighlightBackground": "#008822a1", //Other SEARCH MATCHES
    "editor.selectionBackground": "#aa3800",
    "editor.selectionForeground": "#224411",
    "editor.selectionHighlightBackground": "#bb4900",
    "editorWhitespace.foreground": "#444444",
    "statusBar.background": "#663366",
    "statusBar.foreground": "#cac",
    "statusBar.noFolderBackground": "#663333",
    "statusBar.debuggingBackground": "#263238",
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.labelFormat": "short",
  "workbench.iconTheme": "simple-icons",
  "workbench.settings.editor": "json",
  "workbench.sideBar.location": "left",
  "workbench.statusBar.visible": true,
  "zenMode.fullScreen": true,

  // Per Stack settings:
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
  },
  "[python]": {
    "editor.tabSize": 4,
  },

  // Plugin - GitLens
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressUpdateNotice": true,
    "suppressWelcomeNotice": true
  },
  "gitlens.blame.heatmap.enabled": true,
  "gitlens.currentLine.enabled": true,
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.keymap": "alternate",
  "gitlens.views.lineHistory.enabled": true,

  // Plugin - Kubernetes
  "vs-kubernetes": {
    "vs-kubernetes.helm-path.linux": "/home/user/.vs-kubernetes/tools/helm/linux-amd64/helm",
    "vs-kubernetes.draft-path.linux": "/home/user/.vs-kubernetes/tools/draft/linux-amd64/draft",
    "vs-kubernetes.kubectl-path.linux": "/home/user/.vs-kubernetes/tools/kubectl/kubectl",
    "vs-kubernetes.minikube-path.linux": "/home/user/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vscode-kubernetes.helm-path.linux": "/home/user/.vs-kubernetes/tools/helm/linux-amd64/helm",
    "vscode-kubernetes.minikube-path.linux": "/home/user/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vscode-kubernetes.kubectl-path.linux": "/home/user/.vs-kubernetes/tools/kubectl/kubectl"
  },

  // Plugin - LiveServer
  "liveServer.settings.donotShowInfoMsg": true,

  // Plugin - Settings Sync
  "sync.gist": "7fc97ccd1eb6cec364eec3e2bb3d7290",
  "sync.quietSync": false,
  "sync.removeExtensions": true,
  "sync.syncExtensions": true,
  "sync.autoDownload": false,
  "sync.autoUpload": false,
  "sync.forceDownload": false,

  // SortLines
  "sortLines.filterBlankLines": false,
  "sortLines.sortEntireFile": true,

  // Stack JavaScript
  "js/ts.implicitProjectConfig.checkJs": true,
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",

  // Stack JavaScript - TypeScript
  "typescript.suggest.autoImports": true,
  "typescript.suggest.paths": false, // required by hristian-kohler.path-intellisense
  "typescript.tsserver.log": "verbose",
  // "typescript.updateImportsOnFileMove.enabled": "never",

  // Stack Python
  "python.languageServer": "Pylance",

  // Theme - Icon - Material
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.showWelcomeMessage": false,
  "material-icon-theme.files.associations": {
    "ormconfig.json": "database",
    "tsconfig.json": "tune",
    "*.proto": "3d"
  },
  "material-icon-theme.folders.associations": {
    "infra": "app",
    "entities": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "fakes": "mock",
    "websockets": "pipe",
    "protos": "pipe",
    "grpc": "pipe"
  },
  "snippets.snippetsLocation": "/home/user/.config/Code/User/globalStorage/tahabasri.snippets/data.json",
  "redhat.telemetry.enabled": true,
}
